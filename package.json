{
  "name": "ng-monaco-editor",
  "version": "2.3.0",
  "description": "Angular wrapper for monaco-editor",
  "repository": "git+https://github.com/alauda/ng-monaco-editor.git",
  "author": "Alauda Frontend",
  "contributors": [
    "Peng Xiao <pengxiao@outlook.com>",
    "JounQin <admin@1stG.me> (https://www.1stG.me)"
  ],
  "license": "MIT",
  "scripts": {
    "build": "rimraf release && ng-packagr -c .",
    "lint": "run-p lint:*",
    "lint:es": "eslint . --cache --ext md,js,ts --max-warnings 10",
    "lint:ts": "tslint -p . -t stylish",
    "lint:tsc": "tsc --noEmit",
    "postinstall": "yarn-deduplicate || exit 0",
    "prepublishOnly": "yarn build",
    "start": "yarn storybook",
    "storybook": "start-storybook -p 9001 -s node_modules/monaco-editor/dev",
    "storybook:build": "rimraf demo && build-storybook -s node_modules/monaco-editor/min -o demo"
  },
  "peerDependencies": {
    "@angular/core": ">=11.0.0",
    "lodash-es": ">=4.0.0",
    "monaco-editor": ">=0.20.0"
  },
  "dependencies": {
    "css-element-queries": "^1.2.3"
  },
  "devDependencies": {
    "@1stg/common-config": "^1.2.5",
    "@1stg/tslint-config": "^1.0.0",
    "@angular-devkit/build-angular": "^0.1102.1",
    "@angular/cli": "^11.2.1",
    "@angular/common": "^11.2.3",
    "@angular/compiler": "^11.2.3",
    "@angular/compiler-cli": "^11.2.3",
    "@angular/core": "^11.2.3",
    "@angular/forms": "^11.2.3",
    "@angular/platform-browser": "^11.2.3",
    "@angular/platform-browser-dynamic": "^11.2.3",
    "@storybook/addon-actions": "^6.1.20",
    "@storybook/addon-knobs": "^6.1.20",
    "@storybook/addon-options": "^5.3.21",
    "@storybook/angular": "^6.1.20",
    "@storybook/cli": "^6.1.20",
    "@types/lodash-es": "^4.17.4",
    "@types/requirejs": "^2.1.32",
    "lodash-es": "^4.17.21",
    "monaco-editor": "^0.22.3",
    "ng-packagr": "^11.2.4",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "tslib": "^2.1.0",
    "tslint": "^6.1.3",
    "typescript": "^4.2.2",
    "yarn-deduplicate": "^3.1.0",
    "zone.js": "^0.11.4"
  },
  "resolutions": {
    "prettier": "^2.2.1"
  }
}
